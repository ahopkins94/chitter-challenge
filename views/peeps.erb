<h1> Chitter App! </h1>
<h1> <%= flash[:notice] %> <h1>
  <form action="/sign_up" method="get">
    <input type="submit" value="sign up">
  </form>
  <form action="/sign_in" method="get">
    <input type="submit" value="sign in">
  </form>
  <ul id="peeps">
    <% @peeps.each do |peep| %>
    <li>
      <%= peep.name %>, <%= peep.username %>: <%= peep.post %>: <%= peep.time %>
    </li>
    <% end %>
  </ul>
